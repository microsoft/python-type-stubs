# pyright: strict

from faker.providers import BaseProvider
from random import Random
from typing import Any, Callable, NoReturn, Optional, Sequence, Union

random: Random
mod_random: Random

class Generator:
    providers: Any = ...
    def __init__(self, **config: Any) -> None: ...
    def add_provider(self, provider: Any) -> None: ...
    def provider(self, name: str) -> Union[BaseProvider, None]: ...
    def get_providers(self) -> Sequence[BaseProvider]: ...
    @property
    def random(self) -> Random: ...
    @random.setter
    def random(self, value: Random) -> NoReturn: ...
    def seed_instance(self, seed: Optional[Any] = ...) -> NoReturn: ...
    @classmethod
    def seed(cls, seed: Optional[Any] = ...) -> NoReturn: ...
    def format(self, formatter: str, *args: Any, **kwargs: Any) -> Any: ...
    def get_formatter(self, formatter: str) -> Callable[..., Any]: ...
    def set_formatter(self, name: Any, method: Any) -> NoReturn: ...
    def parse(self, text: str) -> str: ...

